{
    "name": "wai-ui",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "vue-cli-service serve",
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "dev2": "webpack-dev-server --config build/webpack.config.js",
        "build2": "webpack --config build/webpack.config.js",
        "dev3": "webpack-box dev",
        "build3": "webpack-box build",
        "dll3": "webpack-box dll",
        "build:ssr3": "webpack-box build:ssr",
        "ssr:server3": "webpack-box ssr:server"
    },
    "resolve": {
        "@": "./src"
    },
    "dependencies": {
        "async-validator": "^3.2.3",
        "core-js": "^3.4.3",
        "gulp-concat": "^2.6.1",
        "gulp-cssmin": "^0.2.0",
        "gulp-postcss": "^8.0.0",
        "gulp-rename": "^2.0.0",
        "vue": "^2.6.10"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.1.0",
        "@vue/cli-service": "^4.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "node-sass": "^7.0.0",
        "postcss-salad": "^2.0.1",
        "precss": "^4.0.0",
        "raw-loader": "^4.0.0",
        "sass-loader": "7.1.0",
        "sass-resources-loader": "1.3.3",
        "svg-sprite-loader": "^4.1.6",
        "vue-router": "^3.1.3",
        "vue-svg-loader": "^0.15.0",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "postcss": {
        "plugins": {
            "autoprefixer": {},
            "postcss-salad": {
                "browsers": [
                    "ie > 8",
                    "last 2 versions"
                ],
                "features": {
                    "bem": {
                        "shortcuts": {
                            "component": "b",
                            "descendent": "e",
                            "modifier": "modifier"
                        },
                        "separators": {
                            "descendent": "__",
                            "modifier": "_"
                        }
                    }
                }
            }
        }
    }
}
